{
  "name": "@liam/db-structure",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server.js"
  },
  "dependencies": {
    "@bjorn3/browser_wasi_shim": "^0.3.0",
    "@ruby/prism": "1.2.0",
    "pg-query-emscripten": "5.1.0",
    "valibot": "^1.0.0-beta.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@liam/configs": "workspace:*",
    "@pgsql/types": "15.0.2",
    "lodash-es": "4.17.21",
    "peggy": "4.1.1",
    "pluralize-esm": "9.0.5",
    "typescript": "5",
    "vitest": "2.1.4"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "gen:parser": "./scripts/generateParser.sh",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest --watch=false",
    "cp:prism": "cp node_modules/@ruby/prism/src/prism.wasm src/parser/schemarb-prism/prism.wasm"
  },
  "types": "dist/index.d.ts"
}
