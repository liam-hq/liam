{
  "name": "@liam-hq/db",
  "private": true,
  "version": "0.0.0",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "6.4.1",
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.49.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@liam-hq/configs": "workspace:*",
    "prisma": "6.4.1",
    "supabase": "2.15.8",
    "typescript": "5"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:prisma": "prisma generate",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "lint:tsc": "tsc --noEmit",
    "migrate:dev": "sh ./scripts/migrate-dev.sh",
    "migrate:status": "prisma migrate status",
    "supabase:start": "pnpm supabase start",
    "supabase:stop": "pnpm supabase stop"
  }
}
