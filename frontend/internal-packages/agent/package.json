{
  "name": "@liam-hq/agent",
  "private": true,
  "version": "0.1.0",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "dependencies": {
    "@langchain/community": "0.3.53",
    "@langchain/core": "0.3.72",
    "@langchain/langgraph": "0.4.8",
    "@langchain/langgraph-checkpoint": "0.1.1",
    "@langchain/openai": "0.6.9",
    "@liam-hq/artifact": "workspace:*",
    "@liam-hq/db": "workspace:*",
    "@liam-hq/neverthrow": "workspace:*",
    "@liam-hq/pglite-server": "workspace:*",
    "@liam-hq/schema": "workspace:*",
    "@valibot/to-json-schema": "1.3.0",
    "dotenv": "16.5.0",
    "fast-json-patch": "3.1.1",
    "langsmith": "0.3.66",
    "neverthrow": "8.2.0",
    "p-limit": "7.1.1",
    "uuid": "11.1.0",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@liam-hq/configs": "workspace:*",
    "@types/node": "22.18.0",
    "@types/uuid": "10.0.0",
    "eslint": "9.34.0",
    "msw": "2.10.5",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "yoctocolors": "2.1.2"
  },
  "scripts": {
    "dev": "tsc --watch",
    "execute-deep-modeling": "pnpm test:integration src/createGraph.integration.test.ts",
    "execute-deep-modeling:debug": "LOG_LEVEL=DEBUG pnpm test:integration src/createGraph.integration.test.ts",
    "execute-deep-modeling:warn": "LOG_LEVEL=WARN pnpm test:integration src/createGraph.integration.test.ts",
    "execute-design-process": "pnpm test:integration src/db-agent/createDbAgentGraph.integration.test.ts",
    "execute-design-process:debug": "LOG_LEVEL=DEBUG pnpm test:integration src/db-agent/createDbAgentGraph.integration.test.ts",
    "execute-design-process:warn": "LOG_LEVEL=WARN pnpm test:integration src/db-agent/createDbAgentGraph.integration.test.ts",
    "execute-qa-agent": "pnpm test:integration src/qa-agent/createQaAgentGraph.integration.test.ts",
    "execute-qa-agent:debug": "LOG_LEVEL=DEBUG pnpm test:integration src/qa-agent/createQaAgentGraph.integration.test.ts",
    "fmt": "concurrently \"pnpm:fmt:*\"",
    "fmt:biome": "biome check --write --unsafe .",
    "fmt:eslint": "eslint --fix .",
    "lint": "concurrently \"pnpm:lint:*\"",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest --watch=false --passWithNoTests",
    "test:integration": "vitest --watch=false --passWithNoTests --config vitest.config.integration.ts"
  }
}
