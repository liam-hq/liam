{
  "name": "@liam-hq/pglite-server",
  "private": true,
  "version": "0.0.1",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@electric-sql/pglite": "0.3.10",
    "@liam-hq/schema": "workspace:*",
    "@pgsql/types": "15.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@liam-hq/configs": "workspace:*",
    "eslint": "9.37.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "scripts": {
    "fmt": "concurrently \"pnpm:fmt:*\"",
    "fmt:biome": "biome check --write --unsafe .",
    "fmt:eslint": "eslint --fix .",
    "lint": "concurrently \"pnpm:lint:*\"",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest"
  }
}
