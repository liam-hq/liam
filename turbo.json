{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist", "dist-cli"]
    },
    "@liam-hq/erd-core#build": {
      "dependsOn": ["^build", "gen"],
      "outputs": ["dist"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@liam-hq/cli#dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "@liam-hq/erd-web#dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "gen": {
      "dependsOn": ["^gen"]
    },
    "lint": {
      "dependsOn": ["gen", "^build", "^lint"]
    },
    "fmt": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
